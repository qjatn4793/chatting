{
  "name": "chatting-front",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "description": "Desktop Chatting App",
  "author": "kbs",
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "axios": "^1.7.9",
    "cra-template": "1.2.0",
    "jwt-decode": "^4.0.0",
    "net": "^1.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0",
    "react-scripts": "5.0.1",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "electron-pack": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.chatting.app",
    "productName": "ChattingApp",
    "files": [
      "build/**/*",
      "electron.js"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "publish": null
    },
    "linux": {
      "publish": null
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12"
  }
}
